<?xml version="1.0" encoding="UTF-8"?>
<faces-config xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
	version="2.2">
	<navigation-rule>
		<from-view-id>jsp3.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>pagesuivante</from-outcome>
			<to-view-id>jsp3Result.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
		<from-action>#{mbean1.testErreur}</from-action>
			<from-outcome>erreur</from-outcome>
			<to-view-id>jsp3Error.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
</faces-config>
